{
    "name": "hakam/multi-tenancy-bundle",
    "description": "Symfony bundle to extend doctrine to support db switcher and multi tenants",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramy hakam",
            "email": "pencilsoft1@gmail.com"
        }
    ],

    "require": {
        "php": " ^8.0 | ^8.1",
        "symfony/config": " ^5.4.1 | ^6.0.1 | ^6.1|^6.2|^6.3",
        "symfony/dependency-injection": "  ^5.4.1| ^6.0.1 | ^6.1.1|^6.2|^6.3",
        "symfony/http-kernel": "  ^5.4.1| ^6.0.1 | ^6.1.1|^6.2|^6.3",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "symfony/orm-pack": "^2.0",
        "doctrine/annotations": "^1.10",
        "symfony/filesystem": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Hakam\\MultiTenancyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hakam\\MultiTenancyBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4@dev | ^6.1|^6.2|^6.3",
        "phpunit/phpunit": "^9.5"
    }
}
